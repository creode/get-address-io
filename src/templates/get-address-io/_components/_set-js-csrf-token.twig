{% if nonce is not defined %}
    {% set nonce="" %}
{% endif %}

<script nonce="{{ nonce }}">
    if (typeof csrfToken == 'undefined') {
        var csrfToken = {
            csrfTokenName: "{{ craft.app.config.general.csrfTokenName|e('js') }}",
            csrfTokenValue: "{{ craft.app.request.csrfToken|e('js') }}"
        };
    }
</script>
